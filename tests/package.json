{
    "name": "users-demo-tests",
    "version": "1.0.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "author": "Vamsi Krishna Darbhamulla",
    "scripts": {
        "clean": "rm -rf ./dist/**",
        "api-build": "tsc -b --verbose",
        "api-itest": "ts-mocha --paths --timeout 5000 ./api/*.api.spec.ts",
        "api-scripts": "npm install && export TEST_TYPE=api && ts-mocha --paths --timeout 5000 ./api/*.api.spec.ts --reporter mocha-multi-reporters --reporter-options configFile=reporterconfig.js",
        "cy:run": "cypress run",
        "cy:open": "cypress open",
        "prettier-check": "prettier --config ./.prettierrc './**/*.ts' --check",
        "prettier-format": "prettier --config ./.prettierrc './**/*.ts' --write",
        "lint": "eslint --ext .ts,.js .",
        "clean:reports": "rm -R -f ./reports && mkdir -p ./reports/mocha",
        "pretest": "npm run clean && npm run clean:reports",
        "combine-reports": "mochawesome-merge ./reports/mocha/**/*.json > ./reports/mocha/report.json",
        "generate-report": "marge ./reports/mocha/report.json -f report -o ./reports/mocha",
        "posttest": "npm run combine-reports && npm run generate-report",
        "ui-scripts": "cypress run --env TEST_TYPE=ui --reporter Cypress-multi-reporters --reporter-options \"configFile=reporterconfig.js\"",
        "ui-test": "npm run pretest && npm run ui-scripts && npm run posttest",
        "api-test": "npm run pretest && npm run api-scripts && npm run posttest"
    },
    "files": [
        "/dist"
    ],
    "pre-commit": [
        "prettier-format"
    ],
    "devDependencies": {
        "@types/chai": "4.2.21",
        "@types/mocha": "9.0.0",
        "@types/uuid": "8.3.0",
        "@typescript-eslint/eslint-plugin": "4.28.5",
        "@typescript-eslint/parser": "4.28.5",
        "cypress": "9.5.0",
        "cypress-multi-reporters": "1.5.0",
        "cypress-terminal-report": "3.4.1",
        "eslint": "7.31.0",
        "eslint-plugin-chai-friendly": "0.7.1",
        "eslint-plugin-cypress": "2.12.1",
        "mocha": "9.2.1",
        "mocha-junit-reporter": "2.0.2",
        "mochawesome": "6.2.2",
        "mochawesome-merge": "4.2.0",
        "mochawesome-report-generator": "5.2.0",
        "pre-commit": "1.2.2",
        "prettier": "2.5.1",
        "qs": "6.10.2",
        "@types/qs": "6.9.7",
        "typescript": "4.5.5",
        "uuid": "8.3.2",
        "bluebird": "3.7.2",
        "@types/bluebird": "3.5.36",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-airbnb-typescript": "16.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-import-resolver-typescript": "2.5.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-mocha": "9.0.0",
        "eslint-plugin-mocha-no-only": "1.1.1",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "^7.0.4",
        "@faker-js/faker": "5.5.3",
        "@types/faker": "5.5.9",
        "axios": "0.26.0",
        "ts-mocha": "8.0.0",
        "ts-node": "10.5.0",
        "chai": "4.3.4",
        "node": "16.5.0",
        "@types/node": "15.0.2",
        "mocha-multi-reporters": "1.5.1"
    }
}
